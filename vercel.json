{
  "buildCommand": "cd frontend && npm install --include=dev && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://ivan-reseller-backend-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
