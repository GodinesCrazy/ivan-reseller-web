{
  "framework": null,

  "buildCommand": "cd frontend && npm install && npm install tailwindcss postcss autoprefixer && npm run build",

  "outputDirectory": "frontend/dist",

  "installCommand": "cd frontend && npm install",

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://ivan-reseller-backend-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
