{
  "name": "ivan-reseller-backend",
  "version": "1.0.0",
  "description": "Ivan Reseller Backend - Node.js + TypeScript + Express API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:with-migrations": "npx prisma migrate deploy && node dist/server.js",
    "start:prod": "prisma migrate deploy && node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "provision:credentials": "tsx scripts/provision-credentials.ts",
    "verify": "tsx scripts/verify-config.ts",
    "configure-apis": "tsx scripts/configure-apis-from-file.ts",
    "configure-and-test": "tsx scripts/configure-and-test-apis.ts",
    "test-apis": "tsx scripts/test-apis.ts",
    "test:connectivity": "tsx src/scripts/test-aliexpress-connectivity.ts",
    "diagnose:connectivity": "tsx scripts/diagnose-connectivity.ts",
    "run:ebay-real-cycle": "tsx src/scripts/run-ebay-real-cycle.ts"
  },
  "keywords": [
    "dropshipping",
    "ecommerce",
    "api",
    "typescript"
  ],
  "author": "Ivan Reseller",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@slack/web-api": "^7.12.0",
    "@sparticuz/chromium": "^110.0.1",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "bullmq": "^5.1.0",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "discord.js": "^14.24.2",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "on-headers": "^1.1.0",
    "prisma": "^5.7.0",
    "puppeteer": "^24.28.0",
    "puppeteer-core": "^24.28.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^5.9.0",
    "socket.io": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsx": "^4.7.0",
    "twilio": "^5.10.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
